{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:netlify": "VITE_USE_NETLIFY_FUNCTIONS=true vite",
    "dev:php": "VITE_USE_NETLIFY_FUNCTIONS=false vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:netlify": "VITE_USE_NETLIFY_FUNCTIONS=true vite build",
    "build:php": "VITE_USE_NETLIFY_FUNCTIONS=false vite build",
    "build:static": "vite build --mode static",
    "build:vercel": "VITE_USE_NETLIFY_FUNCTIONS=false vite build",
    "build:analysis": "ANALYZE=true vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:php": "VITE_USE_NETLIFY_FUNCTIONS=false vite preview",
    "preview:netlify": "VITE_USE_NETLIFY_FUNCTIONS=true vite preview",
    "functions:dev": "netlify functions:serve",
    "functions:build": "netlify functions:build",
    "deploy:netlify": "npm run build:netlify && netlify deploy --prod",
    "deploy:preview": "npm run build:netlify && netlify deploy",
    "deploy:vercel": "npm run build:vercel && vercel --prod",
    "setup:env": "cp .env.example .env.local",
    "setup:netlify": "npm run setup:env && npm install -g netlify-cli",
    "setup:php": "npm run setup:env && echo 'VITE_USE_NETLIFY_FUNCTIONS=false' >> .env.local",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json && npm install",
    "type-check": "tsc --noEmit",
    "test:build": "npm run build && npm run preview"
  },
